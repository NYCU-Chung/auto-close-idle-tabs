{
  "manifest_version": 3,
  "name": "AutoClose Idle Tabs",
  "version": "1.1",
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": ["<all_urls>"],
  "icons": { "48": "icon.png" }
}
