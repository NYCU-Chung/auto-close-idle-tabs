{
  "manifest_version": 3,
  "name": "AutoClose Idle Tabs",
  "version": "1.1.1",
  "permissions": ["tabs", "storage", "notifications", "contextMenus", "idle"],
  "host_permissions": ["<all_urls>"],
  "background": { "service_worker": "background.js" },
  "action": { "default_popup": "popup.html" },
  "icons": { "48": "icon.png" }
}
